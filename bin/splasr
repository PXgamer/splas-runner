#!/usr/bin/env php
<?php
if (file_exists(__DIR__ . '/../../../autoload.php')) {
    require __DIR__ . '/../../../autoload.php';
} else {
    require __DIR__ . '/../vendor/autoload.php';
}
$app = new Symfony\Component\Console\Application('Splas Runner', '2.0.5');
$app->add(new \pxgamer\SplasRunner\RunCommand());
$app->run();